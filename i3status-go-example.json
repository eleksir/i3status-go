// OpenVPN status. Note that directive status in openvpn.conf should point to
// state file and *update* its timestamp at least as frequent as mtime_threshold in seconds
"vpn": {
	"enabled": false,
	"statusfile" : "/tmp/openvpn-status.stat",
	"mtime_threshold": 10,
	// Color in html notation
	"down_color" : "red",
	"up_color" : "green",

	"separator"	: {
		"left": {
			"enabled": true,
			"color": "#3e78fd",
			"background": "#000000",
			"symbol": "â–¶",
			"font": "Noto Sans",
			// â€˜xx-smallâ€™, â€˜x-smallâ€™, â€˜smallâ€™, â€˜mediumâ€™, â€˜largeâ€™, â€˜x-largeâ€™, â€˜xx-largeâ€™,  or one of relative sizes
			// â€˜smallerâ€™ or â€˜largerâ€™
			"font_size": "medium"
		},

		"right": {
			"enabled": true,
			"color": "#3e78fd",
			"background": "#000000",
			"symbol": "â—€",
			"font": "Noto Sans",
			// â€˜xx-smallâ€™, â€˜x-smallâ€™, â€˜smallâ€™, â€˜mediumâ€™, â€˜largeâ€™, â€˜x-largeâ€™, â€˜xx-largeâ€™,  or one of relative sizes
			// â€˜smallerâ€™ or â€˜largerâ€™
			"font_size": "medium"
		}
	},

	// Also we can tcp-ping some service inside vpn network to ensure that connection is not stoned
	"tcp_check" : {
		"enabled": false,
		"host": "somehost.tld",
		"port": 443,
		"timeout" : 3
	}
},

// Simple sound volume indicator
