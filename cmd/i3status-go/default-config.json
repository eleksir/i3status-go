// Simple sound volume indicator
"simple-volume-pa": {
	"enabled": false,

	// If omitted set to default color defined up here.
	"color": "#3e78fd",

	// If omitted set to default background color defined up here.
	"background": "#000000",

	// If omitted set to default font defined up here.
